    $(document).ready(function () {
        var $container = $('.children'),
        $trigger = $('.nav-parent');  // 分别获取所有的“父子”
        $container.hide();// 隐藏所有的“子”
        $trigger.first().find('.children').show();// 显示第一个“父”的子（展开第一个）
        $trigger.on('click', function (e) {// 当“父”发生点击事件
            //if ($(this).find('.children').is(':hidden')) {
            $trigger.find('.children').hide();// 关闭其他
            $(this).find('.children').show();// 显示自己
            //}
            //e.preventDefault();
        });

    });
    $(document).ready(function () {

        $(".nav-header").each(function (index) {

            $(this).click(function () {
                $(".nav-header").css("color", "white");
                $(".nav-header").eq(index).css("color", "aqua");
            });
        });
        $(".sidebar-nav .nav-list > li > a").each(function (index) {

            $(this).click(function () {
                $(".sidebar-nav .nav-list > li > a").css("background-color", "transparent");
                $(".sidebar-nav .nav-list > li > a").eq(index).css("background-color", "rgba(0,0,0,0.2)");

            });

        });

    });





</script>

