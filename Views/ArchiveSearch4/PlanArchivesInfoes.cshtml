
@model urban_archive.Models.businessPlanProject
<script src="~/Scripts/jquery-2.1.1.min.js" type="text/javascript"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script>

    function Looksaomiao() {


        //var PlanProjSeqNo = document.getElementById("Expr3").value;
        var flag1 = document.getElementById("flag").value;
        var totalSeqNo1 = document.getElementById("totalSeqNo").value;
        if (totalSeqNo1 != null && totalSeqNo1 != null)
        {
            var url = '/ArchiveSearch2/PlanProjView1/?PlanProjSeq=' + totalSeqNo1 + "&flag=" + flag1;
        }

        window.open(url);
    }
</script>

<div style="text-align:center">
    @*<input type="button" id="LookSaomao" name="LookSaomao" onclick="Looksaomiao()" value="查看扫描件" class="btn btn-default" style="padding:6px 12px;font-size:14px;" />*@
    @*disabled="@ViewData["isImage"]"*@
    <input id="flag" value="@ViewData["viewflag"]" hidden="hidden" />
</div>




<div class="form-horizontal">
    <div class="panel panel-default">
        @Html.HiddenFor(model => model.totalSeqNo)

        <table class="table table-bordered dl-horizontal">

            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    工程种类
                </td>
