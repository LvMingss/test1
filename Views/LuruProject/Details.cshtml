                            @Html.DisplayFor(model => model.paperProjectSeqNo)
                        </td>
                        <td style="background-color:#f9f9f9;">
                            工程序号
                        </td>
                        <td>
                            @Html.DisplayFor(model => model.projectNo)
                        </td>
                        <td style="background-color:#f9f9f9;">
