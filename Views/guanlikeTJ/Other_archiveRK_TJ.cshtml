
@{
    ViewBag.Title = "Other_archiveRK_TJ";
}


<h2><i class="fa fa-tags kongge"></i>其他档案入库统计（只能查询新档案）</h2>
<hr />

@section css{
    <link type="text/css" rel="stylesheet" href="~/Scripts/jquery/jquery-ui.css">
}
@section scripts{
    <script type="text/javascript" src="~/Scripts/jquery/jquery-1.8.3.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery.ui.core.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery.ui.datepicker.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery.ui.widget.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery-ui-timepicker-addon.js"></script>
    @*<script type="text/javascript" src="~/Scripts/jquery/jquery-ui-timepicker-zh-CN.js"></script>*@
    <script>
        $(function () {
            $("#startdata").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
        $(function () {
            $("#enddata").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });

        $(function () {
            $("#startdata1").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
        $(function () {
            $("#enddata1").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });

        $(function () {
            $("#startdata2").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
        $(function () {
            $("#enddata2").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });

        $(function () {
            $("#startdata3").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
        $(function () {
            $("#enddata3").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
    </script>
}
@using (Html.BeginForm())
{


    <div style="font-size:20px;">
        <i class="fa fa-caret-right" style="padding-right:5px;"></i>请输入入库日期（通用）
    </div><br>
    <div class="panel panel-default">
        <table class="table table-bordered dl-horizontal">

            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    起始日期:
                </td>
                <td style="width:26%;">
                    <input type="text" id="startdata" name="startdata" class="form-control">
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    终止日期:
                </td>
                <td>
                    <input type="text" id="enddata" name="enddata" class="form-control">
                </td>
            </tr>
            <tr>
                <td colspan="3" style="text-align:right;">
                    <input type="submit" id="ok" value="查看统计结果" name="action" class="btn btn-default" />
                </td>

            </tr>
        </table>
    </div>

    <div style="font-size:20px;">
        <i class="fa fa-caret-right" style="padding-right:5px;"></i>请输入入库日期（规划）
    </div><br>
    <div class="panel panel-default">
        <table class="table table-bordered dl-horizontal">

            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    起始日期:
                </td>
                <td>
                    <input type="text" id="startdata1" name="startdata1" class="form-control">
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    终止日期:
                </td>
                <td>
                    <input type="text" id="enddata1" name="enddata1" class="form-control">
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    区域:
                </td>
                <td>
                    @Html.DropDownList("quyu1", null, new { @class = "form-control", @style = "display:inline" })
                </td>
            </tr>
            <tr>
                <td colspan="5" style="text-align:center;">
                    <input type="submit" id="ok1" value="查看统计结果(规划)" name="action" class="btn btn-default" />
                </td>

            </tr>
        </table>
    </div>

    <div style="font-size:20px;">
        <i class="fa fa-caret-right" style="padding-right:5px;"></i>请输入入库日期（执照）
    </div><br>
    <div class="panel panel-default">
        <table class="table table-bordered dl-horizontal">

            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    起始日期:
                </td>
                <td>
                    <input type="text" id="startdata2" name="startdata2" class="form-control">
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    终止日期:
                </td>
                <td>
                    <input type="text" id="enddata2" name="enddata2" class="form-control">
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    区域:
                </td>
                <td>
                    @Html.DropDownList("quyu2", null, new { @class = "form-control", @style = "display:inline" })
                </td>
            </tr>
            <tr>
                <td colspan="5" style="text-align:center;">
                    <input type="submit" id="ok2" value="查看统计结果(执照)" name="action" class="btn btn-default" />
                </td>

            </tr>
        </table>
    </div>

    <div style="font-size:20px;">
        <i class="fa fa-caret-right" style="padding-right:5px;"></i>请输入入库日期（规划）
    </div><br>
    <div class="panel panel-default">
        <table class="table table-bordered dl-horizontal">

            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    起始日期:
                </td>
                <td style="width:27%;">
                    <input type="text" id="startdata3" name="startdata3" class="form-control">
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    终止日期:
                </td>
                <td style="width:27%;">
                    <input type="text" id="enddata3" name="enddata3" class="form-control">
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    类型:
                </td>
                <td>
                    @Html.DropDownList("type1", null, "全部" ,new { @class = "form-control", @style = "display:inline" })
                </td>
            </tr>
            <tr>
                <td colspan="5" style="text-align:center;">
                    <input type="submit" id="ok3" value="查看统计结果(规划 类型)" name="action" class="btn btn-default" />
                </td>
            </tr>
        </table>
    </div>
}
