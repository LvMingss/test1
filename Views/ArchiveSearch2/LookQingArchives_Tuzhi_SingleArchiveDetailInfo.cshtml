
@model urban_archive.Models.TuzhiArchives
<script src="~/Scripts/jquery-2.1.1.min.js" type="text/javascript"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<meta http-equiv="Content-Type" content="text/html; charset=GB2312" />
<script>
    function Looksaomiao() {

        var seqNo1 = document.getElementById("seqNo").value;
        while (seqNo1.length< 4)
        {
            seqNo1 = "0" + seqNo1;
        }
        var url = '/ArchiveSearch2/TuzhiArchivesView1/?location=' + seqNo1;
        window.open(url);
    }
</script>

<div style="text-align:center">
    <input id="Msg" value="@ViewData["Message"]" style="border:0px;color:coral;background:#ecf0f5;height:25px;font-size:14px;" />
    <input type="button" id="Looksaomiao" name="Looksaomiao" onclick="Looksaomiao()" value="查看扫描件" class="btn btn-default" style="padding:6px 12px;font-size:14px;" disabled="@ViewData["isImage"]" />
</div>



<div class="form-horizontal">
    <div class="panel panel-default">
        @Html.HiddenFor(model => model.seqNo)
        <table class="table table-bordered dl-horizontal">
            
            
            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    序号
                </td>
                <td >
                    @Html.DisplayFor(model => model.seqNo, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.seqNo, "", new { @class = "text-danger" })
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    分类号
                </td>
                <td>
                    @Html.DisplayFor(model => model.classNo, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.classNo, "", new { @class = "text-danger" })
                </td>
            </tr>
            
            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    档号
                </td>
                <td>
                    @Html.DisplayFor(model => model.archiveNo, new { htmlAttributes = new { @class = "form-control" } })
