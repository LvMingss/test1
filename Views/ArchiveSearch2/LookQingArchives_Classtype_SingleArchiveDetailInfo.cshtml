<script>
    $(document).ready(function () {

        $("#LookSaomao").click(function () {
            var ArchiveNo1 = document.getElementById("archiveNo").value;
           

            $.ajax({
                type: "POST",
                url: "/ArchiveSearch2/LookQingArchivesClass/?ArchiveNo="+ArchiveNo1,
                contentType: "application/json",
                data: "",
                dataType: "json",
                success: function (data) {

                    var url1 = "/ArchiveSearch2/OtherArchivesView1/?location="+ data;
                    window.open(url1);


                }

            });
        });
    });
</script>

