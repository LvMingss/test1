                </tr>
                <tr>
                    <td style="background-color:#f9f9f9;width:10%;">
                        计划竣工时间
                    </td>
                    <td colspan="3">
                        @Html.DisplayFor(model => model.planningEndDate)
                    </td>

                </tr>
                <tr>
                    <td style="background-color:#f9f9f9;width:10%;">
                        声像科责任人
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.csyjPerson)
                    </td>
                    <td style="background-color:#f9f9f9;width:10%;">
                        初审日期
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.csyjDate)
                    </td>

                </tr>
                <tr>
                    <td style="background-color:#f9f9f9;width:10%;">
                        初审意见
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.csyj)
                    </td>
                    @*<td style="background-color:#f9f9f9;width:10%;">
                        经办人
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.operater)
                    </td>*@

                </tr>
                <tr>
                    <td style="background-color:#f9f9f9;width:10%;">
                        录像带盒数
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.videoCassetteBoxCount)
                    </td>
                    <td style="background-color:#f9f9f9;width:10%;">
                        照片盒数
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.photoBoxCount)
                    </td>

                </tr>
