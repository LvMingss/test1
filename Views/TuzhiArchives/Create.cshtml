<script type="text/javascript" src="~/Scripts/jquery/jquery-ui-timepicker-addon.js"></script>

<script>
            $(function () {
                $("#luruTime").datepicker({
                    changeMonth: true,
                    changeYear: true
                });
            });
            $(function () {
                $("#jgDate").datepicker({
                    changeMonth: true,
                    changeYear: true
                });
            });
    function openclass() {
        number = window.showModalDialog('/ProjectInfoes/ZTree',"", 'dialogWidth=300px;dialogHeight=500px');
        //弹出模型对话框，obj为传值参数,name为子页面返回参数
        document.getElementById("classNo").value = number;
    }
    function ifok() {
        var msg = "确认要提交吗?";
        if (confirm(msg) == true) {
            return true;
        } else {
            return false;
        }
    }
</script>
