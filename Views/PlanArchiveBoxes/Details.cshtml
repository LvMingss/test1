@model urban_archive.Models.PlanArchiveBox

@{
    ViewBag.Title = "Details";
}
<h2><i class="fa fa-tags kongge"></i>规划档案案卷查看</h2>
<style type="text/css">
    .jubu {
        max-width: 100%;
    }
</style>

@*<script type="text/javascript" src="~/Scripts/jquery/jquery-ui-timepicker-zh-CN.js"></script>*@
<script>

            function save() {
                var msg = "是否确认完成该案卷修改";
                if (confirm(msg) == true) {
                    return true;
                } else {
                    return false;
                }
            }

</script>
