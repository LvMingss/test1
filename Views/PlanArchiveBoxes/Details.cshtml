                        共
                        @Html.EditorFor(model => model.pageCnt, new { htmlAttributes = new { @class = "form-control", @style = "display:inline;width:50px;", @ReadOnly = "true" } })
                        @Html.ValidationMessageFor(model => model.pageCnt, "", new { @class = "text-danger" })
                        页，
                        其中图纸
                        @Html.EditorFor(model => model.drawing, new { htmlAttributes = new { @class = "form-control", @style = "display:inline;width:50px;", @ReadOnly = "true" } })
                        @Html.ValidationMessageFor(model => model.drawing, "", new { @class = "text-danger" })
                        页
                    </td>   
                    <td style="font-weight:bold;">
                        案卷厚度
                    </td>
                    <td>
                        @Html.DropDownList("ArchiveThick", null, new { @class = "form-control", @ReadOnly = "true" })
                    </td>                 
                </tr>               
                
                <tr>
                    <td style="font-weight:bold;">
                        接受日期
                    </td>
                    <td>
                        @Html.EditorFor(model => model.receiveTime, new { htmlAttributes = new { @class = "form-control", @ReadOnly = "true" } })
                        @Html.ValidationMessageFor(model => model.receiveTime, "", new { @class = "text-danger" })
                    </td>
                    <td style="font-weight:bold;">
                        录入日期
                    </td>
                    <td>
                        @Html.EditorFor(model => model.luruTime, new { htmlAttributes = new { @class = "form-control", @ReadOnly = "true" } })
                        @Html.ValidationMessageFor(model => model.luruTime, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td style="font-weight:bold;">
                        是否为异地
                    </td>
                    <td>
                        @Html.EditorFor(model => model.isYD, new { htmlAttributes = new { @class = "form-control", @ReadOnly = "true" } })
                        @Html.ValidationMessageFor(model => model.isYD, "", new { @class = "text-danger" })
                    </td>
                    <td style="font-weight:bold;">
                        是否为内部
                    </td>
                    <td>
                        @Html.DropDownList("isNeibu", null, new { @class = "form-control", @ReadOnly = "true" })
                    </td>
                </tr>
            </table>
        </div>
        <div style="text-align:center;">
            <p>
                @Html.ActionLink("编辑", "Edit", new { id = Model.ID }, new { @class = "btn btn-default" })
                @Html.ActionLink("返回", "Index", new { status = Model.status.Trim(), classifyID = Model.classifyID }, new { @class = "btn btn-default" })
            </p>
        </div>
    </div>
