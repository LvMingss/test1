<body style="background:#fff;">
<div class="content" style="overflow:auto;height:100%;">
<h2><i class="fa fa-tags"></i>工程信息导出</h2>
<hr />
<script>
    function closewindow()
    {
        window.close()
    }
</script>
@using (Html.BeginForm())
{

    <form id="form2">
        <div style="font-size:20px;">
            <i class="fa fa-caret-right"></i>请选择工程类型
        </div><br>
        <div class="panel panel-default">
            <table class="table table-bordered dl-horizontal">
                <tr>
                    <td style="background-color:#f9f9f9;width:25%;">
                        工程类型:
                    </td>
                    <td>
                        @Html.DropDownList("classifyID", null, new { @class = "form-control", @style = "display:inline" })
                    </td>
                </tr>
            </table>
        </div>
        <div style="font-size:20px;">
            <i class="fa fa-caret-right"></i>请输入工程序号的范围
        </div>

        <br>
        <div class="panel panel-default">
            <table class="table table-bordered dl-horizontal">
                <tr>
                    <td style="background-color:#f9f9f9;width:25%;">
                        起始工程序号:
                    </td>
                    <td>
                        <input type="text" name="seqNoStart" id="seqNoStart" class="form-control " />
                    </td>
                    <td style="background-color:#f9f9f9;width:25%;">
                        终止工程序号:
                    </td>
                    <td>
                        <input type="text" name="seqNoEnd" id="seqNoEnd" class="form-control " />
                    </td>
                </tr>
                <tr>
                    <td colspan="4" style="text-align:center;">
                        <input type="submit" name="action" class="btn btn-default" id="ok" value="工程信息导出（工程序号）" />
                        <input type="button" class="btn btn-default" id="cancel" value="取消" onclick="closewindow()" />

                    </td>

                </tr>
            </table>
        </div>
        <div style="font-size:20px;">
            <i class="fa fa-caret-right"></i>请输入项目顺序号的范围
        </div>
        <br>
        <div class="panel panel-default">
            <table class="table table-bordered dl-horizontal">
                <tr>
                    <td style="background-color:#f9f9f9;width:25%;">
                        起始项目顺序号:
                    </td>
                    <td>
                        <input type="text" name="txtSeqNoS" id="txtSeqNoS" class="form-control " />
                    </td>
                    <td style="background-color:#f9f9f9;width:25%;">
                        终止项目顺序号:
                    </td>
                    <td>
                        <input type="text" name="txtSeqNoE" id="txtSeqNoE" class="form-control " />
                    </td>
                </tr>
                <tr>
                    <td colspan="4" style="text-align:center;">
                        <input type="submit" name="action" class="btn btn-default" id="ok" value="工程信息导出（项目顺序号）" />
                        <input type="button" class="btn btn-default" id="cancel" value="取消" onclick="closewindow()">

                    </td>

                </tr>
            </table>
        </div>

    </form>
}      
</div>
</body>
</html>