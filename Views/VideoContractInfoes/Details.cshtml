@model urban_archive.Models.VideoContractInfo

@{
    ViewBag.Title = "Details";
}
<section>
    <h2 style="float:left;"><i class="fa fa-tags kongge"></i>移交责任书信息</h2>
    <ol class="breadcrumb">
        <li class="active">声像档案</li>
        <li class="active">前期材料</li>
        <li>移交责任书管理</li>
    </ol>
    <div style="clear:both;"></div>
</section>
<hr style="margin-top:0px;"/>
 @using (Html.BeginForm())
 {
    <div class="panel panel-default">
        <table class="table table-bordered @*table-striped*@">
            <tr>
                <td style="background-color:#f9f9f9;min-width:120px;width:6%;">
                    责任书编号
                </td>
                <td style="width:27%;">
                    @Html.DisplayFor(model => model.contractNo)
                </td>
                <td style="background-color:#f9f9f9;min-width:120px;width:6%;">
                    签订日期
                </td>
                <td style="width:27%;">
                    @Html.DisplayFor(model => model.dateSigned)
                </td>
                <td style="background-color:#f9f9f9;min-width:120px;width:6%;">
                    移交方
                </td>
                <td style="width:28%;">
                    @Html.DisplayFor(model => model.transferUnit)
                </td>

            </tr>


            <tr>
                <td style="background-color:#f9f9f9;">
                    工程名称
                </td>
                <td>
                    @Html.DisplayFor(model => model.projectName)
                </td>
                <td style="background-color:#f9f9f9;">
                    工程地点
                </td>
                <td>
                    @Html.DisplayFor(model => model.location)
                </td>
                <td style="background-color:#f9f9f9;">
                    层数
                </td>
                <td>@Html.DisplayFor(model => model.layerCount)</td>
            </tr>

            <tr>
                <td style="background-color:#f9f9f9;">
                    建筑面积
                </td>
                <td>
                    @Html.DisplayFor(model => model.buildingArea)
                </td>
                <td style="background-color:#f9f9f9;">
                    计划开工时间
                </td>
                <td>
                    @Html.DisplayFor(model => model.planningStartDate)
                </td>
                <td style="background-color:#f9f9f9;">
                    计划竣工时间
                </td>
                <td>
                    @Html.DisplayFor(model => model.planningEndDate)
                </td>
            </tr>
            <tr>
                <td style="background-color:#f9f9f9;">
                    工程地点分区
                </td>
                <td>
                    @Html.DisplayFor(model => model.constructArea)
                </td>
            </tr>

        </table>
    </div>
    <div class="panel panel-default">
        <table class="table table-bordered @*table-striped*@">
            <tr>
                <td colspan="8" style="background-color:#f9f9f9;">
                    甲方<i class="fa fa-caret-down"></i>
                </td>
            </tr>
            <tr>
                <td style="background-color:#f9f9f9;min-width:40px;width:6%;">
                    地址
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partAaddress)
                </td>
                <td style="background-color:#f9f9f9;min-width:100px;width:6%;">
                    法定代表人
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partALegalRepresent)
                </td>
                <td style="background-color:#f9f9f9;min-width:100px;width:6%;">
                    委托代理人
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partAweituoAgent)
                </td>
                <td style="background-color:#f9f9f9;min-width:80px;width:6%;">
                    联系电话
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partAcontactTel)
                </td>
            </tr>
        </table>
    </div>
    <div class="panel panel-default">
        <table class="table table-bordered @*table-striped*@">
            <tr>
                <td colspan="8" style="background-color:#f9f9f9;">
                    乙方<i class="fa fa-caret-down"></i>
                </td>
            </tr>
            <tr>
                <td style="background-color:#f9f9f9;min-width:40px;width:6%;">
                    地址
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partBadress)
                </td>
                <td style="background-color:#f9f9f9;min-width:100px;width:6%;">
                    法定代表人
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partBLegalRepresent)
                </td>
                <td style="background-color:#f9f9f9;min-width:100px;width:6%;">
                    委托代理人
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partBweituoAgent)
                </td>
                <td style="background-color:#f9f9f9;min-width:80px;width:6%;">
                    联系电话
                </td>
                <td style="width:19%;">
                    @Html.DisplayFor(model => model.partBcontactTel)
                </td>
            </tr>
        </table>
    </div>

