
@{
    ViewBag.Title = "打印执照档案（执照号范围）";
}
<h2><i class="fa fa-tags kongge"></i>打印执照档案（执照号范围）</h2>
<hr />

@using (Html.BeginForm())
{
    <div style="font-size:20px;">
        <i class="fa fa-caret-right kongge"></i>查询方式（一）请输入执照号范围（精确查找）
    </div><br>
    <div class="panel panel-default">
        <table class="table table-bordered dl-horizontal">
            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    起始执照号:
                </td>
                <td>
                    <input type="text" name="licenceNoS" id="licenceNoS" class="form-control " />
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    终止执照号:
                </td>
                <td>
                    <input type="text" name="licenceNoE" id="licenceNoE" class="form-control " />
                </td>
            </tr>            
            <tr>
                <td colspan="4" style="text-align:center;">
                    <input type="submit" name="action" class="btn btn-default" id="ok" value="打印执照档案（精确）" />
                    <input type="submit" name="action" class="btn btn-default" id="cancel" value="取消" />
                </td>

            </tr>
        </table>
    </div>
    <hr />
    <div style="font-size:20px;">

        <i class="fa fa-caret-right"></i>查询方式（二）请输入年度范围（精确查找）
    </div><br>
    <div class="panel panel-default">
        <table class="table table-bordered dl-horizontal">
            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    起始年份:
                </td>
                <td>
                    <input type="text" name="yearNoS" id="licenceNoS" class="form-control " />
                </td>
                <td style="background-color:#f9f9f9;width:10%;">
                    终止年份:
                </td>
                <td>
                    <input type="text" name="yearNoE" id="licenceNoE" class="form-control " />
                </td>
            </tr>
            <tr>
                <td colspan="4" style="text-align:center;">
                    <input type="submit" name="action" class="btn btn-default" id="ok" value="打印执照档案（年份）" />
                    <input type="submit" name="action" class="btn btn-default" id="cancel" value="取消" />
                </td>

            </tr>
        </table>
    </div>
    <div style="font-size:20px;">
        <i class="fa fa-caret-right"></i> 查询方式（三）请输入执照号（模糊查找）
    </div><br>
    <div class="panel panel-default">
        <table class="table table-bordered dl-horizontal">
            <tr>
                <td style="background-color:#f9f9f9;width:10%;">
                    输入执照号:
                </td>
                <td colspan="3">
                    <input type="text" name="licenceNo" id="licenceNo" class="form-control " />
                </td>
                
            </tr>
            <tr>
                <td colspan="4" style="text-align:center;">
                    <input type="submit" name="action" class="btn btn-default" id="ok" value="打印执照档案（模糊）" />
                    <input type="submit" name="action" class="btn btn-default" id="cancel" value="取消" />
                    <input type="submit" name="action" class="btn btn-default" id="all" value="打印全部" />
                </td>

            </tr>
        </table>
    </div>

