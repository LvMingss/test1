@model urban_archive.Models.ContractInfo
@{
    ViewBag.Title = "Create";
}
@section css{
    <link type="text/css" rel="stylesheet" href="~/Scripts/jquery/jquery-ui.css">
}
<style type="text/css">
    .jubu {
        max-width: 100%;
    }
</style>
@section scripts{
    <script type="text/javascript" src="~/Scripts/jquery/jquery-1.8.3.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery.ui.core.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery.ui.datepicker.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery.ui.widget.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery/jquery-ui-timepicker-addon.js"></script>


    <script>
        $(function () {
            $("#dateSigned").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
        $(function () {
            $("#planningStartDate").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
        $(function () {
            $("#planningEndDate").datepicker({
                changeMonth: true,
                changeYear: true
            });
        });
        $().ready(function () {
            $("#contractNo").blur(function () {
                var ss = $("#contractNo").val();
                if (ss.length == 0) {
                    alert("责任书编号不能为空");
                }
                else {
                    if (ss.length < 7) {
                        alert("责任书编号不能少于7位！");
                    }
                }
            });
        });

        $().ready(function () {
            $("#projectName").blur(function () {
                var ss = $("#projectName").val();
                if (ss.length == 0) {
                    alert("工程名称不能为空");
                }
            });
        });
        function saveok() {
            var msg = "确认提交保存吗?";
            if (confirm(msg) == true) {
                return true;
            } else {
                return false;
            }
        }
    </script>
}
<section>
    <h2 style="float:left;"><i class="fa fa-tags kongge"></i>签订移交责任书</h2>
    <ol class="breadcrumb">
        <li class="active">竣工档案</li>
        <li class="active">前期材料</li>
        <li>移交责任书管理</li>
    </ol>
    <div style="clear:both;"></div>
</section>

