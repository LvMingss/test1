                <input type="button" id="ProjectContent" value="打印工程目录" class="btn btn-default" style="@ViewData["ProjectContent"]" />

                <input type="button" id="fengpiG" value="打印工程封皮(公开)" class="btn btn-default" style="@ViewData["fengpiG"]" />
